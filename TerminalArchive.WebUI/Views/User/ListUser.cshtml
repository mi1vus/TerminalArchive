@model IEnumerable<TerminalArchive.Domain.Models.User>

@{
    ViewBag.Title = "Пользователи";
}

<h2>Пользователи</h2>

<p>
    @Html.ActionLink("Создать пользователя", "AddOrEdit")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Pass)
        </th>*@
        <th>Редактировать</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Pass)
        </td>*@
        <td>
            @Html.ActionLink("Редактировать", "AddOrEdit", new { id=item.Id })
        </td>
    </tr>
}

</table>
